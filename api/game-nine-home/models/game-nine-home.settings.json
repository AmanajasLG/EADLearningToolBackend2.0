    "farFrom": {
      "type": "enumeration",
      "enum": [
        "Banco",
        "Escola",
        "Farmacia",
        "Supermercado",
        "Hospital",
        "Parque",
        "Restaurante",
        "Shopping"
      ]
    },
    "icon": {
      "model": "file",
      "via": "related",
