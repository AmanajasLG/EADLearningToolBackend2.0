{
  "kind": "collectionType",
  "collectionName": "tags",
  "info": {
    "name": "Tag"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "type": {
      "type": "enumeration",
      "enum": [
        "category",
        "color",
        "time",
        "weather"
      ]
    },
    "cover": {
      "type": "enumeration",
      "enum": [
        "fullbody",
        "over",
        "under"
      ]
    }
  }
}
